{
  "name": "ahmadsajid1989/zammadapibundle",
  "type": "symfony-bundle",
  "description": "Zammad API Client for Symfony2",
  "keywords": ["zammad", "zammad api client symfony","zammad api symfony2",  "symfony"],
  "homepage": "https://github.com/ahmadsajid1989/zammadapibundle",
  "license": "MIT",
  "authors": [{
    "name": "Ahmad Sajid",
    "email": "ahmadsajid1989@gmail.com"
  },{
    "name": "Main PHP Library Contributor",
    "homepage": "https://github.com/zammad/zammad-api-client-php"
  }],
  "require": {
    "php": ">=5.3.3",
    "zammad/zammad-api-client-php": "1.2.*"
  },

  "suggest": {
    "symfony/console": "If you want to use commands to interact with the client"
  },
  "autoload": {
    "psr-0": { "ahmadsajid1989\\ZammadApiBundle": "" }
  },
  "target-dir": "ahmadsajid1989/ZammadApiBundle",
  "config": {
    "bin-dir": "bin/"
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "dev-master"
    }
  }
